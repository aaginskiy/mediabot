{
  "name": "mediabot",
  "description": "Media management automation suite.",
  "version": "0.2.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Artem Aginskiy",
    "email": "aaginskiy@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src/",
    "test": "test/api/"
  },
  "engines": {
    "node": ">= 14.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "jest": "jest --coverage",
    "test:ci": "standard --fix && NODE_ENV=test jest --verbose true --silent --detectOpenHandles --coverage && cat ./coverage/lcov.info | coveralls",
    "jest:watch": "jest --watch --verbose false",
    "release": "standard-version",
    "lint": "standard --fix",
    "start:watch": "NODE_ENV=production nodemon src/",
    "start": "NODE_ENV=production node src/",
    "dev:watch": "NODE_ENV=development nodemon src/",
    "dev": "NODE_ENV=development node src/",
    "test": "jest"
  },
  "dependencies": {
    "@feathersjs/authentication-oauth": "^4.5.4",
    "@feathersjs/configuration": "^4.5.3",
    "@feathersjs/errors": "^4.5.3",
    "@feathersjs/express": "^4.5.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio": "^4.5.4",
    "@mediabot/tmdb": "aaginskiy/tmdb",
    "body-parser": "^1.18.2",
    "commitizen": "^4.1.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "feathers-hooks-common": "^4.11.1",
    "feathers-memory": "^2.1.3",
    "feathers-nedb": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.2",
    "glob-promise": "^3.2.0",
    "global": "^4.4.0",
    "got": "^8.3.1",
    "helmet": "^3.9.0",
    "lerna": "^3.20.2",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "nedb": "^1.8.0",
    "nodemon": "^2.0.4",
    "serve-favicon": "^2.4.5",
    "shellwords": "^0.1.1",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "winston": "^3",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.27",
    "coveralls": "^3.0.0",
    "git-cz": "^4.7.0",
    "jest": "^26",
    "jest-chain": "^1.0.5",
    "jest-circus": "^23.6.0",
    "jest-extended": "^0.11.0",
    "nock": "^9.2.5",
    "nyc": "^11.2.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "standard": "^11.0.1",
    "standard-version": "^4.4.0"
  },
  "jest": {
    "watchman": false,
    "setupTestFrameworkScriptFile": "jest-extended",
    "coverageReporters": [
      "text-summary",
      "lcov",
      "html"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}