{
  "name": "mediabot",
  "description": "Media management automation suite.",
  "version": "0.2.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Artem Aginskiy",
    "email": "aaginskiy@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 14.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:ci": "NODE_ENV=test jest --verbose true --silent --coverage",
    "test:watch": "jest --watch --verbose false",
    "start:watch": "NODE_ENV=production nodemon lib/",
    "start": "NODE_ENV=production node lib/",
    "dev:watch": "NODE_ENV=development ts-node-dev --no-notify src/",
    "dev": "NODE_ENV=development node lib/",
    "compile": "shx rm -rf lib/ && tsc",
    "commit": "yarn compile && yarn git-cz --disable-emoji",
    "lint": "eslint src/. test/. --config .eslintrc.js --ext .ts --fix"
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/configuration": "^4.5.3",
    "@feathersjs/errors": "^4.5.3",
    "@feathersjs/express": "^4.5.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio": "^4.5.4",
    "@feathersjs/transport-commons": "^4.5.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "got": "^11.5.2",
    "helmet": "^3.23.3",
    "lodash": "^4.17.19",
    "node-themoviedb": "^0.2.4",
    "serve-favicon": "^2.5.0",
    "winston": "^3.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.9",
    "@types/serve-favicon": "^2.5.0",
    "@types/streamtest": "^1.2.0",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "axios": "^0.19.2",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "git-cz": "^4.7.0",
    "jest": "^26.2.2",
    "jest-chain": "^1.0.5",
    "jest-circus": "^26.2.2",
    "jest-extended": "^0.11.0",
    "nock": "^13.0.3",
    "shx": "^0.3.2",
    "streamtest": "^2.0.0",
    "ts-jest": "^26.1.4",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.6"
  }
}
