{
  "name": "@mediabot/mediabot",
  "description": "Media management automation suite.",
  "version": "0.2.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Artem Aginskiy",
    "email": "aaginskiy@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": ">= 14.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "docs": "jsdoc -c .jsdoc.js",
    "typedoc": "typedoc --out docs src",
    "test": "jest --coverage",
    "test:ci": "NODE_ENV=test jest --verbose true --silent --coverage && coveralls < coverage/lcov.info",
    "test:watch": "jest --watch --verbose false",
    "start:watch": "NODE_ENV=production nodemon lib/",
    "start": "NODE_ENV=production node lib/",
    "dev:watch": "NODE_ENV=development ts-node-dev --files --no-notify --project ./tsconfig.json -r tsconfig-paths/register src/",
    "dev": "NODE_ENV=development node lib/",
    "compile": "shx rm -rf lib/ && tsc",
    "lint": "eslint src/. --config .eslintrc.js --ext .ts --fix"
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/configuration": "^4.5.3",
    "@feathersjs/errors": "^4.5.3",
    "@feathersjs/express": "^4.5.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio": "^4.5.4",
    "@feathersjs/transport-commons": "^4.5.7",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "change-case": "^4.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "eslint": "^7.4.0",
    "fast-glob": "^3.2.4",
    "feathers-hooks-common": "^5.0.4",
    "feathers-memory": "^4.1.0",
    "feathers-nedb": "^5.1.0",
    "fp-ts": "^2.9.0",
    "git-cz": "^4.7.0",
    "got": "^11.8.0",
    "helmet": "^3.23.3",
    "io-ts": "^2.2.13",
    "jest": "^26.2.2",
    "jest-circus": "^26.2.2",
    "langs": "^2.0.0",
    "lodash": "^4.17.19",
    "nedb": "^1.8.0",
    "nock": "^13.0.3",
    "node-themoviedb": "^0.2.4",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.30.5",
    "shellwords": "^0.1.1",
    "shellwords-ts": "^3.0.0",
    "shx": "^0.3.2",
    "winston": "^3.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^1.1.1",
    "tsconfig-paths": "^3.9.0"
  }
}
