{
  "name": "ruby-media-bot",
  "description": "Media management automation suite.",
  "version": "0.1.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Artem Aginskiy",
    "email": "aaginskiy@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src/api/",
    "test": "test/api/"
  },
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "standard --fix && NODE_ENV=test nyc --reporter=text --reporter=html mocha test/api/bootstrap.js test/api/**/*.test.js test/util/**/*.test.js --exit",
    "test:watch": "NODE_ENV=test mocha test/api/bootstrap.js test/api/**/*.test.js test/util/**/*.test.js --watch",
    "jest": "jest",
    "test:ci": "standard --fix && NODE_ENV=test jest --verbose true --silent --detectOpenHandles && cat ./coverage/lcov.info | coveralls",
    "jest:watch": "jest --watch --verbose false",
    "release": "standard-version",
    "lint": "standard --fix",
    "start": "nodemon src/api/"
  },
  "dependencies": {
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.2.2",
    "@feathersjs/express": "^1.1.2",
    "@feathersjs/feathers": "^3.1.0",
    "@feathersjs/socketio": "^3.1.0",
    "@mediabot/tmdb": "aaginskiy/tmdb",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "feathers-hooks-common": "^4.11.1",
    "feathers-logger": "^0.3.1",
    "feathers-memory": "^2.1.3",
    "feathers-nedb": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.2",
    "glob-promise": "^3.2.0",
    "got": "^8.3.1",
    "helmet": "^3.9.0",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "nedb": "^1.8.0",
    "serve-favicon": "^2.4.5",
    "shellwords": "^0.1.1",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "winston": "^3.0.0-rc1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "jest": "^23.6.0",
    "jest-chain": "^1.0.5",
    "jest-circus": "^23.6.0",
    "jest-extended": "^0.11.0",
    "nock": "^9.2.5",
    "nyc": "^11.2.1",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "standard": "^11.0.1",
    "standard-version": "^4.4.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-extended",
    "coverageReporters": ["text-summary", "lcov", "html"]
  }
}
